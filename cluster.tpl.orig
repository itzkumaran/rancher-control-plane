nodes:
  - address: "1.2.3.4" 
    internal_address: "1.2.3.4" 
    user: "${user}" 
    role: ["controlplane"] 
  - address:"{public_ip_2}" 
    internal_address: "${private_ip_2}" 
    user: "${user}" 
    role: ["${role_list}"] 
  - address: "${public_ip_3}" 
    internal_address: "${private_ip_3}" 
    user: "${user}" 
    role: ["${role_list}"]

services:
  etcd:
    snapshot: "${snapshot_flag}" 
    creation: "${snapshot_creation}" 
    retention: "${snapshot_retention}" 

ignore_docker_version: "${ignore_docker_version}" 
